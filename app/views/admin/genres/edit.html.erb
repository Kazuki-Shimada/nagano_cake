<h2 class="bg-light mx-5 w-25">ジャンル編集</h2>
<div class="container text-center">
  <p class="mt-5">ジャンル名</p>
  <%= form_with model: @genre, method: :patch, url: admin_genre_path do |f| %>
    <%= f.text_field :name, class: "form-control" %>
    <div class="form-label">
      <%= f.submit "変更を保存", class: "btn btn-success" %>
    </div>
  <% end %>
</div>
